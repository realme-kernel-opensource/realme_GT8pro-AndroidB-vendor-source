				function = "mdp_esync0_out";
			};

			config_315: config {
				pins = "gpio88";
				drive-strength = <2>;   /* 2 mA */
				bias-pull-down;         /* PULL DOWN */
			};
		};

		sde_esync1_active: sde_esync1_active {
			mux_316: mux {
				pins = "gpio100";
				function = "mdp_esync1_out";
			};

			config_317: config {
				pins = "gpio100";
				drive-strength = <2>;   /* 2 mA */
