				bias-pull-down;         /* PULL DOWN */
			};
		};

		sde_te1_active: sde_te1_active {
			mux_308: mux {
				pins = "gpio87";
				function = "mdp_vsync";
			};

			config_309: config {
				pins = "gpio87";
				drive-strength = <2>;   /* 2 mA */
				bias-pull-down;         /* PULL DOWN */
			};
		};

		sde_te1_suspend: sde_te1_suspend {
			mux_310: mux {
				pins = "gpio87";
				function = "mdp_vsync";
			};

			config_311: config {
				pins = "gpio87";
				drive-strength = <2>;   /* 2 mA */
				bias-pull-down;         /* PULL DOWN */
			};
		};

		sde_te1_function0: sde_te1_function0 {
