&tlmm {
	pmx_sde: pmx_sde {
		sde_dsi_active: sde_dsi_active {
			mux: mux {
				pins = "gpio98";
				function = "gpio";
			};

			config: config {
				pins = "gpio98";
				drive-strength = <8>;   /* 8 mA */
				bias-disable = <0>;   /* no pull */
			};
		};

		sde_dsi_suspend: sde_dsi_suspend {
			mux_298: mux {
				pins = "gpio98";
				function = "gpio";
			};

			config_299: config {
				pins = "gpio98";
				drive-strength = <2>;   /* 2 mA */
				bias-pull-down;         /* PULL DOWN */
			};
		};

		sde_dsi1_active: sde_dsi1_active {
			mux_300: mux {
				pins = "gpio97";
				function = "gpio";
			};

			config_301: config {
				pins = "gpio97";
				drive-strength = <8>;   /* 8 mA */
				bias-disable = <0>;   /* no pull */
			};
		};

		sde_dsi1_suspend: sde_dsi1_suspend {
			mux_302: mux {
				pins = "gpio97";
				function = "gpio";
			};

			config_303: config {
				pins = "gpio97";
				drive-strength = <2>;   /* 2 mA */
				bias-pull-down;         /* PULL DOWN */
			};
		};
	};

	pmx_sde_te: pmx_sde_te {
		sde_te_active: sde_te_active {
			mux_304: mux {
				pins = "gpio86";
				function = "mdp_vsync";
			};

			config_305: config {
				pins = "gpio86";
				drive-strength = <2>;   /* 2 mA */
				bias-pull-down;         /* PULL DOWN */
			};
		};

		sde_te_suspend: sde_te_suspend {
			mux_306: mux {
				pins = "gpio86";
				function = "mdp_vsync";
			};

			config_307: config {
				pins = "gpio86";
