		compatible = "qcom,dsi-ctrl-hfi-hw-v2.10";
		qcom,op-label = "hfi";
		status = "disabled";
		label = "dsi-ctrl-1";
		cell-index = <1>;
		frame-threshold-time-us = <800>;
		reg = <0x0 0x09ac3000 0x0 0x1000>,
			  <0x0 0x09990000 0x0 0x300>;
