	mdss_dsi2: qcom,mdss_dsi_ctrl2@2 {
		compatible = "qcom,dsi-ctrl-hfi-hw-v2.10";
		qcom,op-label = "hfi";
		status = "disabled";
		label = "dsi-ctrl-0";
		cell-index = <0>;
		frame-threshold-time-us = <800>;
		reg = <0x0 0x09ac0000 0x0 0x1000>,
			  <0x0 0x0998f000 0x0 0x300>;
		reg-names = "dsi_ctrl", "mdp_intf_base";
	};

