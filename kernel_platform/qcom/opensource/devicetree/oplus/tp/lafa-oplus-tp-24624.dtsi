&tlmm {
		gpio_aikey_gpio_default: gpio_aikey_gpio_default-state {
			pins = "gpio97";
			function = "gpio";
			drive-strength = <6>;
			bias-disable;
		};

		pmx_ts_int_active {
		ts_int_active: ts_int_active {
			mux {
				pins = "gpio158";
				function = "gpio";
			};

			config {
				pins = "gpio158";
				drive-strength = <8>;
				bias-pull-up;
				input-enable;
			};
		};
	};

	pmx_ts_reset_active {
		ts_reset_active: ts_reset_active {
			mux {
				pins = "gpio159";
				function = "gpio";
			};

			config {
				pins = "gpio159";
				drive-strength = <8>;
				output-high;
				bias-disable;
			};
		};
	};

	pmx_ts_reset_idle {
		ts_reset_idle: ts_reset_idle {
			mux {
				pins = "gpio159";
				function = "gpio";
			};

			config {
				pins = "gpio159";
				drive-strength = <8>;
				output-low;
				bias-disable;
			};
		};
	};
};

&soc {
	devinfo_corvette {
		compatible = "oplus-devinfo";
		pinctrl-names = "default", "sleep", "active", "idle";
		pinctrl-0 = <&hw_pins_default>;
		pinctrl-1 = <&aboard_pin_gpio_sleep>;
		pinctrl-2 = <&aboard_pin_gpio_active>;
		pinctrl-3 = <&aboard_pin_gpio_idle>;

		main-sub {
			use_tristate_gpio;
			compatible = "hw-match, main-sub";
			aboard-gpio0 = <&tlmm 46 0>;
			aboard-patterns = <0 0>;	/*0[high-resistance] 1[pull-down] 2[pull-up]*/
			/*main board*/
			match-projects = <24624 24776>;
		};
	};
	synaptics:synaptics@0 {
		compatible = "synaptics-tcm";
		status = "disabled";

		device,id = <0>;
		device,chip_name = "synaptics-s3910";
		device,resolution = <20224 44480>;

		/*interrupt config*/
		interrupt-parent = <&tlmm>;
		interrupts = <158 0x2008>;
		irq-gpio = <&tlmm 158 0x2008>;

		pinctrl-names = "default",
				"ts_reset_active",
				"ts_reset_idle",
				"ts_resume",
				"ts_suspend",
				"ts_avdd_on",
				"ts_avdd_off";
		pinctrl-0 = <&ts_int_active &ts_reset_active>;
		pinctrl-1 = <&ts_reset_active>;
		pinctrl-2 = <&ts_reset_idle>;
		pinctrl-3 = <&qupv3_se19_spi_mosi_active &qupv3_se19_spi_miso_active &qupv3_se19_spi_clk_active &qupv3_se19_spi_cs_active>;
		pinctrl-4 = <&qupv3_se19_spi_sleep &qupv3_se19_spi_cs_sleep>;
		pinctrl-5 = <&ts_gpio_avdd_on>;
		pinctrl-6 = <&ts_gpio_avdd_off>;

		/*cs active state, not used now*/
		spi,cs_active = <0>;

		/*power config*/
		//power,avdd-supply = <&L22B>;
		//power,avdd_volt = <3000000 3000000>;

		power,vddi-supply = <&L4B>;
		power,vddi_volt = <1800000 1800000>;

		/*bus config*/
		device,attached_bus = <&hbp_spi_bus0>;

		create_with_power_on_support;
	};

	hbp:hbp@0 {
		compatible = "oplus,hbp_core";
		hbp,project = "infiniti";
		hbp,devices = <&synaptics>;
	};
};

