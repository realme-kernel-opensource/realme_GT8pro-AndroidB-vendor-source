#include <dt-bindings/thermal/thermal_qti.h>

&thermal_zones {
	socd {
/* #ifdef OPLUS_FEATURE_CHG_BASIC */
/* polling period change 1s */
		polling-delay-passive = <1000>;
/* #else */
		/* polling-delay-passive = <100>; */
/* #endif */
		thermal-sensors = <&bcl_soc>;

		trips {
			thermal-engine-trip {
				temperature = <100>;
				hysteresis = <0>;
				type = "passive";
			};

			thermal-hal-trip {
				temperature = <90>;
				hysteresis = <0>;
				type = "passive";
			};

			socd_trip: socd-trip {
/* #ifdef OPLUS_FEATURE_CHG_BASIC */
/* thershold: 3% power */
				temperature = <97>;
/* #else */
				/* temperature = <90>; */
/* #endif */
				hysteresis = <0>;
				type = "passive";
			};
		};

		cooling-maps {
			socd_map {
				trip = <&socd_trip>;
				cooling-device =
/* #ifdef OPLUS_FEATURE_CHG_BASIC */
/* change big core: 2.2G, super core 2.4G */
					<&CPU0 THERMAL_NO_LIMIT 11>,
					<&CPU6 THERMAL_NO_LIMIT 15>,
/* #else */
					/* <&APC1_MX_CX_PAUSE 0 1>, */
/* #endif */
					<&cdsp_sw 0 4>,
					<&msm_gpu 0 5>;
			};
		};
	};

	pmih010x-ibat-lvl0 {
		thermal-sensors = <&pmih010x_bcl 0>;

		trips {
			ibat_lvl0:ibat-lvl0 {
				temperature = <13500>;
				hysteresis = <200>;
				type = "passive";
			};
		};
	};

