#include "alor-pm7550ba-pmd802x.dtsi"

&pmic_glink_debug {
	i2c_pmic:i2c@105 {
		reg = <0x105>;  /* I2C instance 5 in ADSP for SE4 */
		#address-cells = <1>;
		#size-cells = <0>;
		qcom,bus-type = "i2c";

		qcom,smb1500@68 {
			compatible = "qcom,i2c-pmic";
			reg = <0x68>;
			qcom,can-sleep;
		};

		qcom,smb1500@69 {
			compatible = "qcom,i2c-pmic";
			reg = <0x69>;
			qcom,can-sleep;
		};
	};
};

&pmic_glink_adc {
	status = "ok";

	smb1500_1_iin {
		reg = <0x1056801>;
		label = "smb1500_1_iin";
	};

	smb1500_1_ichg {
		reg = <0x1056802>;
		label = "smb1500_1_ichg";
	};

	smb1500_1_die_temp {
		reg = <0x1056803>;
		label = "smb1500_1_die_temp";
	};

	smb1500_2_iin {
		reg = <0x1056901>;
		label = "smb1500_2_iin";
	};

	smb1500_2_ichg {
		reg = <0x1056902>;
		label = "smb1500_2_ichg";
	};

	smb1500_2_die_temp {
		reg = <0x1056903>;
		label = "smb1500_2_die_temp";
	};
};
